{
  "name": "md-aggregate",
  "version": "2.2.90",
  "description": "Aggregate README.md with CHANGELOG.md or whatever",
  "keywords": [
    "markdown",
    "cli",
    "aggregate",
    "merge",
    "replace",
    "import"
  ],
  "homepage": "https://github.com/snomiao/js/tree/master/packages/md-aggregate#readme",
  "bugs": {
    "url": "https://github.com/snomiao/js/issues?q=is%3Aissue+is%3Aopen+md-aggregate"
  },
  "repository": "https://github.com/snomiao/js/tree/master/packages/md-aggregate",
  "license": "MIT",
  "author": "Snowstar Miao <snomiao@gmail.com>",
  "type": "module",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs"
  },
  "main": "dist/index.min.cjs",
  "module": "dist/index.min.mjs",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "snobuild",
    "prepack": "npm run build",
    "prepublishOnly": "prettier -w CHANGELOG.md && npm run build && npm link && md-aggregate README.md CHANGELOG.md -w",
    "postversion": "npm publish"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "yargs": "17.6.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "18.8.3",
    "@types/yargs": "17.0.13",
    "prettier": "^2.6.2",
    "snobuild": "^3.3.90"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "ca1c2db9974aa48a03d7695327ba73367192028c"
}
